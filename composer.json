{
    "name": "spotwilliams/phonebook",
    "description": "Simple Phone Book API",
    "type": "project",
    "authors": [
        {
            "name": "Ricardo Vergara",
            "email": "ricardovergarawork@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doctrine/orm": "*",
        "vlucas/phpdotenv": "^4.1",
        "filp/whoops": "^2.7",
        "slim/slim": "4.*",
        "guzzlehttp/psr7": "^1.6",
        "http-interop/http-factory-guzzle": "^1.0",
        "php-di/php-di": "^6.0",
        "league/fractal": "^0.19.2",
        "php-di/slim-bridge": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "PhoneBook\\": "src/",
            "App\\" : "app/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0",
        "phpstan/phpstan": "^0.12.11"
    }
